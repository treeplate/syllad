IntegerFunctionList indexedFunctions(Integer length) {
  IntegerFunctionList result = []:IntegerFunction;
  Integer i = 0;
  while (i < length) {
    Integer j = i;
    Integer getIndex() {
      return j;
    }
    append(result, getIndex);
    i += 1;
  }
  return result;
}

IntegerFunctionList indexes = indexedFunctions(5);
