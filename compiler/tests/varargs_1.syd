String concatList(WhateverList args, Integer offset) {
  if(len(args) - offset == 0) { return ''; }
  if(len(args) - offset == 1) { return concat(args[offset]); }
  return concat(args[offset], concatList(args, offset+1));
}
Null printlnNoSpaces(Anything... args) {
  println(concatList(args, 0));
}
println('Line', '1');
